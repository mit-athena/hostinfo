# $Id: Makefile.in,v 1.3 1999-07-28 21:16:10 danw Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@

all:

check:
	SRCDIR=${top_srcdir} ./regress.sh

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	${INSTALL_SCRIPT} hostinfo.pl ${DESTDIR}${bindir}/hostinfo
	${INSTALL} -m 444 ${srcdir}/hostinfo.1 ${DESTDIR}${mandir}/man1

clean:

distclean: clean
	rm -f config.cache config.log config.status Makefile
